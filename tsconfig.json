{
  "compileOnSave": true,
    "compilerOptions": {
        "incremental": true,
        "target": "es5",
        "module": "esnext",
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "watch": true,
        "types": ["cypress"]
 },
 "include": ["**/*.ts"]
}